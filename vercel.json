{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "./install-rust.sh && pnpm nx build web",
  "installCommand": "pnpm install --frozen-lockfile",
  "outputDirectory": "apps/web/.next",
  "functions": {
    "apps/web/src/app/api/internal/health-check/route.ts": {
      "maxDuration": 30
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
